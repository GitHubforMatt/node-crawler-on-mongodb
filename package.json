{
  "name": "crawler",
  "version": "0.1.0",
  "description": "A crawler repo for hunt ticket.",
  "main": "src/index.js",
  "scripts": {
    "api": "./node_modules/.bin/babel-node ./src/api.js",
    "crawl": "./node_modules/.bin/babel-node ./src/crawl.js",
    "crawl-levels": "./node_modules/.bin/babel-node ./src/crawl-levels.js",
    "crawl-levels:dev": "./node_modules/.bin/babel-node ./src/crawl-levels.js --dev-mode",
    "crawl:dev": "./node_modules/.bin/babel-node ./src/crawl.js --dev-mode",
    "custom": "./node_modules/.bin/babel-node ./src/custom.js",
    "example": "npm run phantomjs ./src/targets/example.back.js --dont-save-data",
    "phantomjs": "./node_modules/phantomjs-prebuilt/bin/phantomjs",
    "relate": "./node_modules/.bin/babel-node ./src/relate.js",
    "start": "npm run crawl && npm run api",
    "test": "echo \"No tests!\"; exit 0 "
  },
  "repository": {
    "type": "git",
    "url": "http://salam@git.liepiaowang.com:81/salam/ticket-crawler.git"
  },
  "keywords": [
    "hunt",
    "ticket",
    "crawler",
    "node",
    "mongodb"
  ],
  "author": "Salam Hiyali",
  "license": "MIT",
  "dependencies": {
    "jaro-winkler": "^0.2.8",
    "mongodb": "^2.2.28",
    "phantomjs-prebuilt": "^2.1.14",
    "restify": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-es2015": "^7.0.0-beta.40",
    "@babel/runtime": "^7.0.0-beta.40",
    "babel-eslint": "^8.2.2",
    "babel-node": "^7.0.0-beta.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "eslint": "^4.18.2"
  }
}
